{
  "acceptedResourceRoles": [
    "*"
  ],
  "constraints": [
    [
      "hostname",
      "CLUSTER",
      ""
    ]
  ],
  "container": {
    "docker": {
      "image": "gitlab/gitlab-ce:9.1.0-ce.0",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 0,
          "labels": {
            "VIP_0": "/gitlab:80"
          },
          "protocol": "tcp",
          "servicePort": 10103,
          "name": "web"
        },
        {
          "containerPort": 443,
          "hostPort": 0,
          "protocol": "tcp"
        },
        {
          "containerPort": 22,
          "hostPort": 0,
          "protocol": "tcp",
          "servicePort": 22222
        },
        {
          "containerPort": 50000,
          "hostPort": 0,
          "protocol": "tcp",
          "servicePort": 50000
        }
      ]
    },
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/var/opt/gitlab",
        "hostPath": "/srv/gitlab/gitlab/opt",
        "mode": "RW"
      },
      {
        "containerPath": "/var/log/gitlab",
        "hostPath": "/srv/gitlab/gitlab/log",
        "mode": "RW"
      },
      {
        "containerPath": "/etc/gitlab",
        "hostPath": "/srv/gitlab-data/gitlab/config",
        "mode": "RW"
      },
      {
        "containerPath": "/gitlab-data",
        "hostPath": "/srv/gitlab-data/gitlab/data",
        "mode": "RW"
      }
    ]
  },
  "cpus": 1,
  "env": {
    "GITLAB_OMNIBUS_CONFIG": " unicorn['worker_processes'] = 2; manage_accounts['enable'] = true; user['home'] = '/gitlab-data/home'; git_data_dir '/gitlab-data/git-data'; gitlab_rails['shared_path'] = '/gitlab-data/shared'; gitlab_rails['uploads_directory'] = '/gitlab-data/uploads'; gitlab_ci['builds_directory'] = '/gitlab-data/builds';"
  },
  "healthChecks": [
    {
      "command": {
        "value": "/opt/gitlab/bin/gitlab-healthcheck --fail > /dev/null"
      },
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 5,
      "protocol": "COMMAND",
      "timeoutSeconds": 20
    }
  ],
  "id": "gitlab",
  "instances": 1,
  "labels": {
    "DCOS_PACKAGE_DEFINITION": "",
    "DCOS_PACKAGE_METADATA": "",
    "DCOS_PACKAGE_NAME": "gitlab",
    "DCOS_PACKAGE_OPTIONS": "e30=",
    "DCOS_PACKAGE_SOURCE": "https://universe.mesosphere.com/repo",
    "DCOS_PACKAGE_VERSION": "1.0-9.1.0",
    "DCOS_SERVICE_NAME": "gitlab",
    "MARATHON_SINGLE_INSTANCE_APP": "true"
  },
  "mem": 2048,
  "upgradeStrategy": {
    "maximumOverCapacity": 0,
    "minimumHealthCapacity": 0
  }
}
